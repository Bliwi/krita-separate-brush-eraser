        <Action name="dninosores_eraser_toggle">
            <icon></icon>
            <text>Toggle Eraser</text>
            <whatsThis>Toggles eraser without switching tool.</whatsThis>
            <toolTip>Toggles eraser without switching tool.</toolTip>
            <iconText></iconText>
            <isCheckable>false</isCheckable>
            <statusTip></statusTip>
        </Action>
    </Actions>
