<?xml version="1.0" encoding="UTF-8"?>
<ActionCollection version="2" name="Scripts">
    <Actions category="Scripts">
        <text>Separate Brush and Eraser</text>

        <Action name="dninosores_activate_brush">
            <icon></icon>
            <text>Switch to Brush</text>
            <whatsThis>Switches to brush tool as if separate from eraser.</whatsThis>
            <toolTip>Switches to brush tool as if separate from eraser.</toolTip>
            <iconText></iconText>
            <isCheckable>false</isCheckable>
            <statusTip></statusTip>
        </Action>

        <Action name="dninosores_activate_eraser">
            <icon></icon>
            <text>Switch to Eraser</text>
            <whatsThis>Switches to brush tool with eraser active as if separate tool.</whatsThis>
            <toolTip>Switches to brush tool with eraser active as if separate tool.</toolTip>
            <iconText></iconText>
            <isCheckable>false</isCheckable>
            <statusTip></statusTip>
        </Action>

        <Action name="dninosores_eraser_on">
            <icon></icon>
            <text>Enable Eraser</text>
